ssageId":"removeConsole","data":{"propertyName":"log"},"desc":"Remove the console.log()."}]},{"ruleId":"no-console","severity":1,"message":"Unexpected console statement. Only these console methods are allowed: warn, error.","line":30,"column":5,"nodeType":"MemberExpression","messageId":"limited","endLine":30,"endColumn":16,"suggestions":[{"fix":{"range":[834,878],"text":""},"messageId":"removeConsole","data":{"propertyName":"log"},"desc":"Remove the console.log()."}]}],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":3,"fixableErrorCount":0,"fixableWarningCount":0,"source":"// Script to reset user password\r\n// Usage: node resetPassword.js <email> <newPassword>\r\n\r\nimport bcrypt from 'bcrypt';\r\nimport mongoose from 'mongoose';\r\nimport User from './models/User.js';\r\nimport 'dotenv/config';\r\n\r\nconst resetPassword = async (email, newPassword) => {\r\n  try {\r\n    // Connect to MongoDB\r\n    await mongoose.connect(process.env.MONGODB_URI);\r\n    console.log('Connected to MongoDB');\r\n\r\n    // Find user\r\n    const user = await User.findOne({ email });\r\n    if (!user) {\r\n      console.error(`User with email ${email} not found`);\r\n      process.exit(1);\r\n    }\r\n\r\n    // Hash new password\r\n    const hashedPassword = await bcrypt.hash(newPassword, 10);\r\n\r\n    // Update password\r\n    user.password = hashedPassword;\r\n    await user.save();\r\n\r\n    console.log(` Password reset successfully for ${email}`);\r\n    console.log(`New password: ${newPassword}`);\r\n    \r\n    process.exit(0);\r\n  } catch (error) {\r\n    console.error('Error resetting password:', error);\r\n    process.exit(1);\r\n  }\r\n};\r\n\r\n// Get arguments\r\nconst email = process.argv[2];\r\nconst newPassword = process.argv[3];\r\n\r\nif (!email || !newPassword) {\r\n  console.error('Usage: node resetPassword.js <email> <newPassword>');\r\n  process.exit(1);\r\n}\r\n\r\nresetPassword(email, newPassword);\r\n","usedDeprecatedRules":[{"